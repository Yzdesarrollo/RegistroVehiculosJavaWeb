<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>tipoVehiculoControlador</servlet-name>
        <servlet-class>Controladores.tipoVehiculoControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VehiculoController</servlet-name>
        <servlet-class>Controladores.VehiculoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tipoCondControlador</servlet-name>
        <servlet-class>Controladores.tipoCondControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contratoControlador</servlet-name>
        <servlet-class>Controladores.contratoControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conductorControlador</servlet-name>
        <servlet-class>Controladores.conductorControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tipoVehiculoControlador</servlet-name>
        <url-pattern>/tipoVehiculoControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VehiculoController</servlet-name>
        <url-pattern>/VehiculoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tipoCondControlador</servlet-name>
        <url-pattern>/tipoCondControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contratoControlador</servlet-name>
        <url-pattern>/contratoControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conductorControlador</servlet-name>
        <url-pattern>/conductorControlador</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
